{
    "name"       : "narrowspark/http-status",
    "type"       : "library",
    "description": " A psr-7 package for working with HTTP statuses.",
    "keywords"   : [
        "psr-7", "psr7", "http", "status", "http-status", "http-status exceptions", "exception"
    ],
    "license"    : "MIT",
    "homepage"   : "https://github.com/narrowspark/http-status",
    "support"    : {
        "issues"      : "https://github.com/narrowspark/http-status/issues",
        "source"      : "https://github.com/narrowspark/http-status"
    },
    "authors"    : [
        {
            "name"    : "Daniel Bannert",
            "email"   : "d.bannert@anolilab.de",
            "homepage": "http://anolilab.de",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                  : "^5.6 || ^7.0"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config"      : "^1.4",
        "phpunit/phpunit"                      : "^5.1"
    },
    "provide": {
        "psr/http-message-implementation"      : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Narrowspark\\HttpStatus\\"        : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Narrowspark\\HttpStatus\\Tests\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                       : "2.0-dev"
        }
    },
    "suggest": {
    },
    "minimum-stability"                        : "dev",
    "prefer-stable"                            : true
}
